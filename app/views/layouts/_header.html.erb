<nav class="">
    <ul>
        <li> <%= link_to "Store Home", store_path %> </li>
        <% if current_user && current_user.current_cart %>
          <li> <%= link_to "Cart", cart_path(current_user.current_cart) %></li>
        <% end %>

        <% if current_user %>
          <h2>Signed in as <%= current_user.email %></h2>
          <br><%= link_to "Sign out", destroy_user_session_path, method: "delete" %></br>
        <% end %>

        
        <% if !user_signed_in? %>
          <li> <%= link_to "sign in", new_user_session_path %></li>
          <li> <%= link_to "Sign up", new_user_registration_path %></li>
        <% end %>
    </ul>
</nav>
